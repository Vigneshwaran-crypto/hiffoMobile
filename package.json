{
  "name": "Hiffo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "run-latest": "react-native run-ios --simulator=\"iPhone 14 Pro Max\"",
    "setup-ios": "npm install --legacy-peer-deps && cd ios && pod install && cd .. && react-native run-ios",
    "setup-android": "npm install --legacy-peer-deps && npx react-native run-android",
    "start": "react-native start",
    "releaseApk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "releaseAab": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "test": "jest",
    "lint": "eslint .",
    "update-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "restart": "react-native run-android && react-native start",
    "jarvis": "npx react-native run-android --port 8088 --deviceId UKRS598HMRUSZPJB && npx react-native start --port 8088"
  },
